{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/app/*"],
      "img/*": ["./public/*"],
      "cmp/*": ["./src/app/@components/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/testAPI/page.jsx", "src/app/@components/workers/CardProfileWorkers.jsx", "src/app/@components/workers/FormBiodata.jsx", "src/app/@components/workers/FormExperience.jsx", "src/app/@components/workers/FormPortofolio.jsx", "src/app/@components/workers/FormSkill.jsx", "src/app/home/page.jsx"],
  "exclude": ["node_modules"]
}
